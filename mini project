import numpy as np
# Create two matrices
A = np.array([[1, 2, 3], [4, 5, 6]])
B = np.array([[7, 8, 9], [10, 11, 12]])
# Perform matrix addition
C = A + B
print(C)
# Perform matrix subtraction
D = A - B
print(D)
# Perform matrix multiplication
E = A * B
print(E)
# Perform matrix division
F = A / B
print(F)
